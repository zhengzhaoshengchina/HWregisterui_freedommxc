define("base/utils/cookie",[],function(){"use strict";var cookieOp={};cookieOp.addCookie=function(name,value,expiresMillis,path){var cookieStr,expiresTime,date;this.hasCookie(name)&&this.delCookie(name);value&&(cookieStr=name+"="+encodeURIComponent(value));expiresTime=Number(expiresMillis);if(!isNaN(expiresTime)&&expiresTime>0){date=new Date;date.setTime(date.getTime()+expiresTime);cookieStr+=path?";path="+path+"; expires="+date.toUTCString():"; expires="+date.toUTCString()}value&&(document.cookie=cookieStr+";secure")};cookieOp.getCookie=function(cname){var i,c,name=cname+"=",ca=document.cookie.split(";");for(i=0;i<ca.length;i++){c=ca[i].trim();if(0==c.indexOf(name))return c.substring(name.length,c.length)}return""};cookieOp.delCookie=function(name){var date=new Date;date.setTime(date.getTime()-1e4);document.cookie=name+"=v; expires="+date.toUTCString()};cookieOp.hasCookie=function(name){return void 0!=this.getCookie(name)};cookieOp.setCookie=function(name,value,expiresMillis){this.addCookie(name,value,expiresMillis)};cookieOp.DEFAULT_EXPIRES_TIME=2592e6;cookieOp.get_cookie=function(e){var n,i,a,t="",r=document.getElementById("JSdev");if(r&&!t)try{n=r.getsid()||"",t=n}catch(o){}try{window.localStorage&&!t&&(n=localStorage[e]||"",t=n)}catch(o){}if(window.navigator.cookieEnabled&&!t){i=document.cookie.indexOf(e+"=");if(-1!=i){i+=e.length+1;a=document.cookie.indexOf(";",i);-1==a&&(a=document.cookie.length),n=decodeURIComponent(document.cookie.substring(i,a))||"",t=n}}return t};return cookieOp});